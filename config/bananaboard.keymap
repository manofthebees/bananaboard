#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>

/ {
    combos {
        compatible = "zmk,combos";

        artsey00 {
            bindings = <&kp M>;
            key-positions = <4 5 6>;
        };
    };

    keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = <
&kp S  &kp T  &kp R  &kp A
&kp O  &kp I  &kp Y  &kp E
            >;
        };
    };
};
